<H3>Profile</H3><br/>
<%= form_for(current_user) do |f| %>
	<%#= render 'shared/error_messages' %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :location %>
	<%= f.text_field :name %>
	<%= f.label :website %>
	<%= f.text_field :name %>
	<%= f.label :area, 'Practice Area' %>
	<%= f.text_field :name %>
	<%= f.label :bio %>
	<%= f.text_area :name, :size => "10x5" %>
	<%= f.submit "Save changes", class: "btn btn-info" %>
<% end %>