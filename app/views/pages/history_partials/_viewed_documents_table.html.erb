<div class="viewed-documents-pane-content">
    <table class="table table-striped">
		<thead>
			<tr>
				<th class="span4">Document</th>
				<th>Jurisdiction</th>
				<th>Practice Area</th>
			</tr>
		</thead>
		<% @viewedforms.each do |f| %>
			<tr>
				<td>
					<%= link_to f.form.name, form_path(f.form) %><br/>
					<div style="font-size: 12px;">
						viewed <%= time_ago_in_words(f.created_at) %> ago
					</div>
				</td>
				<td><%= f.form.jurisdiction %></td>
				<td>
					<% if f.form.practice_area != nil %>
						<%= f.form.practice_area.practice_area_title %>
					<% end %>
				</td>
			</tr>
		<% end %>
	</table>
</div>